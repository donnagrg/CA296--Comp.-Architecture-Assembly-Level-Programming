.586
.model flat,stdcall
.stack 4096
option casemap:none

include     p:\masm32\include\windows.inc
include     p:\masm32\include\kernel32.inc
include		p:\masm32\include\user32.inc
include		p:\masm32\include\msvcrt.inc
include		p:\masm32\include\ca296.inc

includelib p:\masm32\lib\ca296.lib
includelib kernel32.lib
includelib user32.lib
includelib msvcrt.lib

; LINE 39 and LINE 40 solve the next problem

.data
i			DWORD	1
n			DWORD	1
tmp			DWORD	0
.code
	main:nop

		 loopy:
			mov edx, 0
			mov eax, n
			mul i
			mov ecx, 8
			mul ecx
			mov ecx, n
			mov tmp, ecx
			mov n, eax
			cmp edx, 0
			jne done

			;invoke writeInteger, eax
			
			inc i
			jmp loopy
	done:
		dec i
		invoke writeInteger, tmp
		invoke writeInteger, i
		invoke Sleep, 100000000
		invoke ExitProcess,0

	end main
